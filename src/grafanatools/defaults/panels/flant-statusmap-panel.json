{
  "type": "flant-statusmap-panel",
  "title": "Panel Title",
  "gridPos": {
    "x": 0,
    "y": 0,
    "w": 12,
    "h": 9
  },
  "id": 10,
  "targets": [
{
      "format": "time_series",
      "timeColumn": "time",
      "metricColumn": "none",
      "group": [],
      "where": [
        {
          "type": "macro",
          "name": "$__timeFilter",
          "params": []
        }
      ],
      "select": [
        [
          {
            "type": "column",
            "params": [
              "value"
            ]
          }
        ]
      ],
      "rawQuery": false
    }
  ],
  "color": {
    "mode": "spectrum",
    "cardColor": "#b4ff00",
    "colorScale": "sqrt",
    "exponent": 0.5,
    "colorScheme": "interpolateGnYlRd",
    "defaultColor": "#757575",
    "thresholds": []
  },
  "cards": {
    "cardMinWidth": 5,
    "cardVSpacing": 2,
    "cardHSpacing": 2,
    "cardRound": null
  },
  "xAxis": {
    "show": true,
    "showWeekends": true,
    "minBucketWidthToShowWeekends": 4,
    "showCrosshair": true,
    "labelFormat": "%a %m/%d"
  },
  "yAxis": {
    "show": true,
    "showCrosshair": false
  },
  "tooltip": {
    "show": true
  },
  "legend": {
    "show": true
  },
  "data": {
    "unitFormat": "short",
    "decimals": null
  },
  "nullPointMode": "as empty",
  "yAxisSort": "metrics",
  "highlightCards": true,
  "useMax": true
} 
